{
  "name": "red_tetrisboilerplate",
  "version": "0.0.1",
  "author": "redpelicans",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint src",
    "client-dev": "webpack-dev-server --colors --hot --inline --host 0.0.0.0 --port 8080",
    "client-dist": "NODE_ENV=production webpack --progress --colors",
    "srv-dev": "DEBUG=tetris:* babel-watch -w src src/server/main.js",
    "srv-dist": "DEBUG=tetris:* babel src --out-dir dist",
    "test": "mocha --require babel-core/register --reporter spec",
    "coverage": "NODE_ENV=test nyc --include src --all -r text -r lcov -r text --require babel-core/register mocha ",
    "allDev": "npm run srv-dev & npm run client-dev"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "history": "^5.0.0",
    "install": "^0.13.0",
    "koa": "^2.13.0",
    "koa-router": "^9.4.0",
    "npm": "^6.14.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-watch": "^7.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-equal-jsx": "^1.0.9",
    "eslint": "^7.7.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.20.6",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "nyc": {
    "exclude": [
      "build"
    ]
  }
}
